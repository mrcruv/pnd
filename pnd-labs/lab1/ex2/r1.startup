ip addr replace 192.168.100.30/28 dev eth0
iptables -t nat -A POSTROUTING -o eth1 -j MASQUERADE

dpkg -i /var/cache/apt/archives/*.deb
apt install -f udhcpd

echo "nameserver 8.8.8.8" > /etc/resolv.conf

echo "start 192.168.100.17" > /etc/udhcpd.conf
echo "end 192.168.10.29" >> /etc/udhcpd.conf
echo "interface eth0" >> /etc/udhcpd.conf
echo "option subnet 255.255.255.240" >> /etc/udhcpd.conf
echo "option router 192.168.100.30" >> /etc/udhcpd.conf
echo "option lease 43200" >> /etc/udhcpd.conf
echo "option dns 1.1.1.1" >> /etc/udhcpd.conf
echo "option dns 8.8.8.8" >> /etc/udhcpd.conf
echo "option domain local" >> /etc/udhcpd.conf

/etc/init.d/udhcpd start
